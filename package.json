  "main": "server.js",
  "scripts": {
    "start": "npm run css:watch && npx nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "css:watch": "postcss src/public/css/tailwind.css -o src/public/css/style.css",
    "css:build": "cross-env NODE_ENV=production postcss src/public/css/main.css -o src/public/css/style.css",
    "webpack:build": "webpack --build --mode development",
    "webpack:watch": "npx webpack --watch",
    "cpu:profile": "node --prof-process isolate-0xnnnnnnnnnnnn-v8.log > processed.txt",
    "test": "find ./src -name '*.test.js' | xargs mocha -R spec --timeout 10000",
    "build": "NODE_ENV=production npm run css:build",
    "scrape:all": "node src/components/jobs/jobs.service.js",
    "scrape:indeed": "node src/components/jobs/jobs.service.js getIndeedJobs",
    "scrape:so": "node src/components/jobs/jobs.service.js getStackOverflowJobs",
    "scrape:gd": "node src/components/jobs/jobs.service.js getGlassdoorJobs"
  },
